generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model playlists {
  id           String   @id @default(uuid())
  playlistName String
  videos       videos[]
}

model videos {
  id          String    @id @default(uuid())
  title       String
  url         String
  thumb       String
  playlistsId String
  playlists   playlists @relation(fields: [playlistsId], references: [id])
}
