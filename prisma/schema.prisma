generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model videos {
  id         String   @id @default(uuid())
  created_at DateTime @default(now())
  title      String
  url        String
  thumb      String
  playlist   String
}

model users {
  id         String      @id @default(uuid())
  created_at DateTime    @default(now())
  name       String
  github     String
  job        String
  banner     String
  favorites  favorites[]
}

model favorites {
  id      String  @id @default(uuid())
  name    String
  users   users?  @relation(fields: [usersId], references: [id])
  usersId String?
}
